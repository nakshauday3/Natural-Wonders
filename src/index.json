{
   "natural_wonders":[
      {
         "project":"natural wonders of the world"
      },
      {
         "overview":"rest api contains detailed information about some of the beautiful and breathtaking landscapes around the world. Each landscape introduces itself with its name, location and description,followed by landscape properties such as geography, statistics and flora fauna living at the location, every landscape has one picture, and several pictures of the species"
      },
      {
         "index":[
            {
               "landscape_details":[
                  "name",
                  "about",
                  "country"
               ]
            },
            {
               "picture":"landscape.jpeg"
            },
            {
               "Geography":[
                  "lattitude_longitude",
                  "climate",
                  "landscape"
               ]
            },
            {
               "stats":[
                  "yearly_visitors",
                  "likes",
                  "altitude",
                  "unesco_heritage"
               ]
            },
            {
               "species":[
                  "name",
                  "endangered",
                  "picture"
               ]
            }
         ]
      },
      {
         "endpoints":[
            {
               "index":"http://api.naturalwonders.com/"
            },
            {
               "landscapes":"http://api.naturalwonders.com/locations"
            },
            {
               "landscape":"http://api.naturalwonders.com/locations/<name>"
            },
            {
               "landscape_geography":"http://api.naturalwonders.com/locations/<name>/geo"
            },
            {
              "landscape_stats":"http://api.naturalwonders.com/locations/<name>/stats"
            },
            {
              "landscape_picture":"http://api.naturalwonders.com/locations/<name>/pic"
            },
            {
              "species":"http://api.naturalwonders.com/locations/<name>/species"
            },
            {
              "specie_details":"http://api.naturalwonders.com/locations/<name>/species/<specie_name>"
            }
         ]
      },
